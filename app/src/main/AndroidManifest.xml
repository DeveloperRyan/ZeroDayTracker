<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zerodaytracker.zerodaytracker">

    <application
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".BrowseHabitsActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HabitViewActivity"
            android:label="ZeroDayTracker"
            android:parentActivityName="com.zerodaytracker.zerodaytracker.BrowseHabitsActivity">
        </activity>
    </application>

</manifest>